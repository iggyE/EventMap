<Window x:Class="EventMap.Front.View.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EventMap.Front.View"
        mc:Ignorable="d"
        Title="Event Map" Height="500" Width="820">

    <Grid>
        <StackPanel Background="AliceBlue" Margin="0,0,-2,0" Orientation="Horizontal" VerticalAlignment="Top" Height="60">
            <Button Background="AntiqueWhite" Width="100" Height="36" Content="Incoming Events" HorizontalAlignment="Center" Margin="70,0,0,0" />
            <Button Background="AntiqueWhite" Width="100" Height="36" Content="Events" HorizontalAlignment="Center" Margin="70,0,0,0"/>
            <Button Background="AntiqueWhite" Width="100" Height="36" Content="Settings" HorizontalAlignment="Center" Margin="70,0,0,0" />
            <Button Background="AntiqueWhite" Width="100" Height="36" Content="Tutorial" HorizontalAlignment="Center" Margin="70,0,0,0" />
        </StackPanel>

        <Frame Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
